{
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": "warn",
    "linebreak-style": 0,
    "semi": ["error", "never"],
    "import/no-unresolved": 0,
    "no-underscore-dangle": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true,
    "node": true
  },
  "globals": {
    "$": "readonly"
  },
  "extends": [
    "airbnb-base"
  ]
}